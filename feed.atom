---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Ken Pepple</title>
 <link href="http://ken.rabbityard.com/feed.atom" rel="self"/>
 <link href="http://ken.rabbityard.com"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>RENAME</id>
 <author>
   <name>Ken Pepple</name>
 </author>
 {% for post in site.posts limit: 15 %}
 <entry>
   <title type="html">{{ post.title | xml_escape }}</title>
   <link rel="alternate" href="http://ken.rabbityard.com/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>{{ post.id }}</id>
   <content type="xhtml" xml:lang="{% if post.lang %}{{ post.lang }}{% else %}en{% endif %}" xml:base="http://ken.rabbityard.com">
    <div xmlns="http://www.w3.org/1999/xhtml">
     {{ post.content }}
    </div>
   </content>
 </entry>
 {% endfor %}
</feed>