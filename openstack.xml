---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Ken Pepple on OpenStack</title>
 <link href="http://ken.pepple.info/openstack.atom" rel="self"/>
 <link href="http://ken.pepple.info"/>
 <language>en</language>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://ken.pepple.info/</id>
 <author>
   <name>Ken Pepple</name>
   <email>ken.pepple@rabbityard.com</email>
 </author>

 {% for post in site.posts %}
   {% if post.category == "openstack" %}
    <entry>
      <title>{{ post.title }}</title>
      <link href="http://ken.pepple.info{{ post.url }}"/>
      <updated>{{ post.date | date_to_xmlschema }}</updated>
      <id>http://ken.pepple.info/{{ post.id }}</id>
      <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
   {% endif %}
 {% endfor %}

</feed>
